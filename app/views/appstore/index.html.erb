<!-- Votes -->
<div class="row">
  <table class="centered" style="width: 40%;">
    <tr>
      <td class="py-0 pl-0"><h5 id="votes" class="green-text"><%= User.first.get_upvotes.size%></h5></td>
      <td class="py-0 pr-0"><h5 id="votes" class="red-text"><%= @kevin.get_downvotes.size%></h5></td>
    </tr>
    <tr>
      <td class="py-0 pl-0"><a class="btn-floating btn-large waves-effect waves-light green" data-method="put" href="1/upvote">👍</a></td>
      <td class="py-0 pr-0"><a class="btn-floating btn-large waves-effect waves-light red" data-method="put" href="1/downvote">👎</a></td>
    </tr>
  </table>
</div>
